{
  "name": "ganhata",
  "version": "1.0.0",
  "description": "the backend graphql server for ganhata mobile application",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev --name data",
    "production": "npm run generate && npm run migrate && node build/index.js",
    "development": "node build/index.js",
    "prod": "tsc-watch --onSuccess \"npm run production\"",
    "dev": "tsc-watch --onSuccess \"npm run development\""
  },
  "author": "subhashankar behera",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "colors": "^1.4.0",
    "morgan": "^1.10.0",
    "prisma": "^5.5.2",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@prisma/client": "^5.5.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0"
  }
}
